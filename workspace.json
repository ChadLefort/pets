            "index": "apps/dogs/src/index.html",
            "main": "apps/dogs/src/main.tsx",
            "polyfills": "apps/dogs/src/polyfills.ts",
            "tsConfig": "apps/dogs/tsconfig.app.json",
            "assets": [],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
