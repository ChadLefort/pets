{
  "version": 2,
  "projects": {
    "cats": {
      "root": "apps/cats",
      "sourceRoot": "apps/cats/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/cats",
            "index": "apps/cats/src/index.html",
            "main": "apps/cats/src/main.tsx",
            "polyfills": "apps/cats/src/polyfills.ts",
            "tsConfig": "apps/cats/tsconfig.app.json",
            "assets": ["apps/cats/src/favicon.ico", "apps/cats/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cats/src/environments/environment.ts",
                  "with": "apps/cats/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "cats:build",
            "proxyConfig": "./proxy.config.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "cats:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cats/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cats"],
          "options": {
            "jestConfig": "apps/cats/jest.config.js",
            "passWithNoTests": true
          }
        },
        "json-server": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve cats", "npm run json-server"],
            "parallel": true
          }
        }
      }
    },
    "cats-e2e": {
      "root": "apps/cats-e2e",
      "sourceRoot": "apps/cats-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cats-e2e/cypress.json",
            "tsConfig": "apps/cats-e2e/tsconfig.e2e.json",
            "devServerTarget": "cats:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cats:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cats-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "dogs": {
      "root": "apps/dogs",
      "sourceRoot": "apps/dogs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dogs",
            "index": "apps/dogs/src/index.html",
            "main": "apps/dogs/src/main.tsx",
            "polyfills": "apps/dogs/src/polyfills.ts",
            "tsConfig": "apps/dogs/tsconfig.app.json",
            "assets": ["apps/dogs/src/favicon.ico", "apps/dogs/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dogs/src/environments/environment.ts",
                  "with": "apps/dogs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "dogs:build",
            "proxyConfig": "./proxy.config.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "dogs:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dogs/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dogs"],
          "options": {
            "jestConfig": "apps/dogs/jest.config.js",
            "passWithNoTests": true
          }
        },
        "json-server": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve dogs", "npm run json-server"],
            "parallel": true
          }
        }
      }
    },
    "dogs-e2e": {
      "root": "apps/dogs-e2e",
      "sourceRoot": "apps/dogs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dogs-e2e/cypress.json",
            "tsConfig": "apps/dogs-e2e/tsconfig.e2e.json",
            "devServerTarget": "dogs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dogs:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dogs-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "common-ui": {
      "root": "libs/common-ui",
      "sourceRoot": "libs/common-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/common-ui",
            "tsConfig": "libs/common-ui/tsconfig.lib.json",
            "project": "libs/common-ui/package.json",
            "entryFile": "libs/common-ui/src/index.ts",
            "external": ["react", "react-dom", "react-is"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common-ui"],
          "options": {
            "jestConfig": "libs/common-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/common-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/common-ui",
            "config": {
              "configFolder": "libs/common-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "project": "libs/core/package.json",
            "entryFile": "libs/core/src/index.ts",
            "external": ["react", "react-dom"],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "cats"
}
