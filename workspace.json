{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": []
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cats": {
      "root": "apps/cats",
      "sourceRoot": "apps/cats/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/cats",
            "index": "apps/cats/src/index.html",
            "main": "apps/cats/src/main.tsx",
            "polyfills": "apps/cats/src/polyfills.ts",
            "tsConfig": "apps/cats/tsconfig.app.json",
            "assets": [],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "cats:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "cats:build:production"
            }
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve cats", "npm run json-server"],
            "parallel": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cats/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cats"],
          "options": {
            "jestConfig": "apps/cats/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "cats-e2e": {
      "root": "apps/cats-e2e",
      "sourceRoot": "apps/cats-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cats-e2e/cypress.json",
            "tsConfig": "apps/cats-e2e/tsconfig.e2e.json",
            "devServerTarget": "cats:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cats:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cats-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "dogs": {
      "root": "apps/dogs",
      "sourceRoot": "apps/dogs/src",
      "projectType": "application",
      "targets": {
        "build": {
